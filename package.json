{
  "name": "egg-webpack-vue",
  "version": "2.0.0",
  "description": "egg webpack building solution for vue",
  "eggPlugin": {
    "name": "webpackvue",
    "env": [
      "local"
    ]
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "vue",
    "webpack"
  ],
  "devDependencies": {
    "autod": "^3.0.1",
    "egg": "^2.0.0",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.13.1",
    "eslint": "^4.12.0",
    "eslint-config-egg": "^5.1.1",
    "supertest": "^3.0.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "files": [
    "index.js",
    "app.js",
    "agent.js",
    "config",
    "app",
    "lib",
    "builder"
  ],
  "ci": {
    "version": "8, 9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hubcarl/egg-webpack-vue.git"
  },
  "bugs": {
    "url": "https://github.com/hubcarl/egg-webpack-vue/issues"
  },
  "homepage": "https://github.com/hubcarl/egg-webpack-vue#readme",
  "author": "hubcarl@126.com",
  "license": "MIT",
  "dependencies": {

  }
}
